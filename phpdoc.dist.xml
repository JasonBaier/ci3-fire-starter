<?xml version="1.0" encoding="UTF-8" ?>
<phpdoc>
	<title>CodeIgniter v3.0.0 API</title>
    <parser>
        <target>./api/</target>
    </parser>
    <transformer>
        <target>./api/</target>
    </transformer>
    <files>
        <directory>./system</directory>
    </files>

    <logging>
        <level>warn</level>
        <paths>
            <default>./api/log/{DATE}.log</default>
            <errors>./api/{DATE}.errors.log</errors>
        </paths>
    </logging>
</phpdoc>